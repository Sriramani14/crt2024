break statemnet
prime or not

int main() {
    int i,c=0,n;
    scanf("%d",&n);
    
    for(i=2;i<n;i++){
        if(n%i==0){
            c++;
            break;
        }
    }
    if(c==0){
        printf("prime no");
    }
    else{
        printf("not prime  no");
    }
       
    return 0;
}
